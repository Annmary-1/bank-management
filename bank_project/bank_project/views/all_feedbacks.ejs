<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Feedback Data Display</title>
  <style>
    table {
      border-collapse: collapse;
      width: 100%;
      margin-top: 20px;
    }

    table, th, td {
      border: 1px solid #ddd;
    }

    th, td {
      padding: 10px;
      text-align: left;
    }
  </style>
</head>
<body>
  <h1>Feedbacks</h1>
  <table>
    <thead>
      <tr>
        <th>Feedback ID</th>
        <th>User ID</th>
        <th>Feedback Text</th>
        <th>Rating</th>
        <th>Timestamp</th>
      </tr>
    </thead>
    <tbody>
      <% data.forEach(row => { %>
        <tr>
          <td><%= row.feedback_id %></td>
          <td><%= row.user_id %></td>
          <td><%= row.feedback_text %></td>
          <td><%= row.rating %></td>
          <td><%= row.timestamp %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>
