<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Account Data Display</title>
  <style>
    table {
      border-collapse: collapse;
      width: 100%;
      margin-top: 20px;
    }

    table, th, td {
      border: 1px solid #ddd;
    }

    th, td {
      padding: 10px;
      text-align: left;
    }
  </style>
</head>
<body>
  <h1>Details</h1>
  <table>
    <thead>
      <tr>
        <th>User ID</th>
        <th>Full Name</th>
        <th>Account Number</th>
        <th>Email</th>
        <th>Phone Number</th>
        <th>Balance</th>

      </tr>
    </thead>
    <tbody>
      <% data.forEach(row => { %>
        <tr>
          <td><%= row.user_id %></td>
          <td><%= row.full_name %></td>
          <td><%= row.account_number %></td>
          <td><%= row.email %></td>
          <td><%= row.phone_number %></td>
          <td><%= row.balance %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>
